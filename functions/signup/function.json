{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "userProfile",
      "type": "cosmosDB",
      "databaseName": "matchmaker-db",
      "collectionName": "matchmaker-container",
      "createIfNotExists": false,
      "connectionStringSetting": "COSMOSDB_CONNECTION",
      "direction": "out"
    }
  ]
}
